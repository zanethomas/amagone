{
  "manifest_version": 2,
  "name": "amagone",
  "description": "Remove amazon from search results",
  "homepage_url": "https://github.com/zanethomas/amagone",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },
  "permissions": ["storage"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./filter.js"]
  }]
}
