{
  "manifest_version": 2,
  "name": "amagone",
  "description": "Remove amazon from search results",
  "homepage_url": "https://github.com/zanethomas/amagone",
  "version": "1.0",
  "icons": {
    "48": "icons/new-no.png",
    "96": "icons/new-no@2x.png"
  },
  "browser_action": {
    "default_icon": "icons/disabled-38.png",
    "default_title": "amagone"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["storage"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./filter.js"]
  }]
}
